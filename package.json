{
  "name": "@umbranoxio/difflux",
  "version": "0.0.1",
  "description": "A TypeScript library for decompiling and diffing .NET assemblies using ILSpy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "scripts",
    "!dist/**/*.test.*",
    "!dist/test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umbranoxio/difflux.git"
  },
  "bugs": {
    "url": "https://github.com/umbranoxio/difflux/issues"
  },
  "homepage": "https://github.com/umbranoxio/difflux#readme",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "prepack": "npm run build",
    "postinstall": "node scripts/install.js"
  },
  "keywords": [
    "dotnet",
    "decompiler",
    "ilspy",
    "diff",
    "csharp",
    "assembly"
  ],
  "author": "umbranoxio",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "@types/fs-extra": "^11.0.4",
    "@types/diff": "^6.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "node-fetch": "^2.7.0",
    "extract-zip": "^2.0.1",
    "diff": "^7.0.0",
    "tar": "^7.4.3",
    "@types/fs-extra": "^11.0.4",
    "@types/tar": "^6.1.13",
    "@types/diff": "^5.0.9"
  }
} 